{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Actor",
  "description": "An actor from the crawled catalog",
  "type": "object",
  "properties": {
    "id": {
      "description": "Unique identifier for a actor",
      "type": "integer"
    },
    "name": {
      "description": "Name of the actor",
      "type": "string"
    },
    "wiki": {
      "description": "Wikipedia link to see more details about the actor (URL)",
      "type": "string"
    }
  },
  "required": [
    "id",
    "name"
  ]
}
